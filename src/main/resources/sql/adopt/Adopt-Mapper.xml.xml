<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

    <entry key="adoptApplyInsert">
        INSERT INTO SUBSCRIPTION(SUB_NO,MEMBER_NO,SHELTER_NO,ANIMAL_NO,TEL_NUM,NAME,GENDER,ADOPT_REASON,FAMILY_AGREEMENT,WHEN_SICK,BIG_DUTY,WISH_DATE,SUB_READ,SUB_DATE)
        VALUES(SQ_SUBSCRIPTION_PK,?,?,?,?,?,?,?,?,?,?,?,?,?)
    </entry>

    <entry key="shelterNoByName">
        SELECT
        SHELTER_NO
        FROM SHELTER
        WHERE SHELTER_NAME = (SELECT
        careNm
        FROM ANIMAL
        WHERE noticeNo = ?)

    </entry>

</properties>
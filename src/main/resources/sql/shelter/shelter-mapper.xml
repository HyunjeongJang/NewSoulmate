<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

    <entry key="selectShelterList">
        SELECT
        SHELTER_NAME,
        SHELTER_ADDRESS,
        SHELTER_LANDLINE
        FROM SHELTER
        WHERE SHELTER_ADDRESS IS NOT NULL
    </entry>

    <entry key="selectCityAll">
        SELECT *
        FROM CITY
    </entry>

    <entry key="selectVillageAll">
        SELECT *
        FROM VILLAGE
    </entry>

    <entry key="selectVillage">
        SELECT *
        FROM VILLAGE
        WHERE CITY_NO = ?
    </entry>

    <entry key="ShelterListByVillage">
        SELECT
        SHELTER_NO
        SHELTER_NAME
        FROM SHELTER
        WHERE VILLAGE_NO = ?
        WHERE SHELTER_ADDRESS IS NOT NULL
    </entry>



</properties>




